<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="view_partner_application_form_tree" model="ir.ui.view">
            <field name="name">view.partner.application.form.tree.view</field>
            <field name="model">partner.application.form</field>
            <field name="arch" type="xml">
                <tree string="Application">
                    <field name="number" string="Application Number"/>
                     <field name="bvn" />
                    <field name="driver_license"/>
                    <field name="national_id"/>
                    <field name="name" />
                    <field name="last_name" />
                    <field name="city" />
                    <field name="email" />
                    <field name="tin" />
                    <field name="bvn" />
                    <field name="license_no" />
                    <field name="national_id" />
                     <field name="approve_state" />
                </tree>
            </field>
        </record>

        <menuitem action="hr.open_view_employee_list_my" id="hr.menu_open_view_employee_list_my" sequence="3" parent="hr.menu_hr_main" groups="base.group_hr_user"/>

        <!--application form -->
        <record id="view_partner_application_form_form" model="ir.ui.view">
            <field name="name">partner.application.form.form</field>
            <field name="model">partner.application.form</field>

            <field eval="1" name="priority" />
            <field name="arch" type="xml">
                <form string="Partners" >
                    <header>
                        <button name="action_application_submit" string="Submit" type="object" class="oe_highlight" attrs="{'invisible':[('approve_state','in',('review','approve','rejected'))]}" />
                        <button name="action_application_approve" string="Approve" class="oe_highlight"
                            type="object" attrs="{'invisible':[('approve_state','in',('new','approve','rejected'))]}" groups="etax.group_etax_manager"/>

                        <!--<button name="action_application_approve" string="Approve" class="oe_highlight"-->
                            <!--type="object" attrs="{'invisible':[('approve_state','in',('new','approve'))]}" groups="etax.group_etax_manager"/>-->
                        <button name="button_reject" string="Reject" type="object"
                                attrs="{'invisible':[('approve_state','in',('new','approve','rejected'))]}"
                                class="oe_highlight" groups="etax.group_etax_manager"/>

                        <button name="action_application_reject" string="Return To Draft" type="object" attrs="{'invisible':[('approve_state','in',('new','approve'))]}" class="oe_highlight" groups="etax.group_etax_manager"/>

                        <field name="approve_state" widget="statusbar" nolabel="1" statusbar_visible="new,review,approve,rejected" class="oe_link oe_right" />
                    </header>
                    <sheet>
                        <h1>
                            <field name="number" readonly="1" />
                        </h1>
                        <field name="image" widget='image' class="oe_left oe_avatar"
                            options='{"preview_image": "image_medium", "size": [650, 650]}' />
                        <label for="name" />
                        (
                        <field name="is_company"
                            class="oe_inline" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                        <label for="is_company" string="Is a Company?" />
                        )
                        <h1>
                            <field name="name" default_focus="1" placeholder="Name"
                                style="width:40%%" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                        </h1>
                        <div class="oe_title">
                            <div class="oe_edit_only">
                            </div>
                            <group col='4' >
                                <field name="last_name" placeholder="Last Name" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="other_name" placeholder="Other Name" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="date_of_birth" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="sex" required="1" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="marital_state" required="1" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="user_id" required="1" string="User" readonly="1"/>
                                <field name="parent_id" placeholder="Company"
                                    domain="[('is_company', '=', True)]"
                                    context="{'default_is_company': True, 'default_supplier': supplier, 'default_customer': customer}"
                                    attrs="{'invisible': [('is_company','=', True),('parent_id', '=', False)], 'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="category_id" widget="many2many_tags"
                                    placeholder="Tags..." attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                            </group>
                        </div>
                        <group>
                            <group>
                                <label for="type" attrs="{'invisible': [('parent_id','=', False)]}" />
                                <div attrs="{'invisible': [('parent_id','=', False)]}" name="div_type">
                                    <field class="oe_inline" name="type" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                </div>

                                <label for="street" string="Residance Address" />
                                <div>

                                    <field name="street" placeholder="Street..." attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" required="1" />
                                    <field name="street2" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}"/>
                                    <div class="address_format">
                                        <field name="city" placeholder="City" style="width: 40%%"
                                               attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" required="1"/>
                                        <field name="state_id" class="oe_no_button" placeholder="State"
                                            style="width: 37%%" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" required="1" />
                                        <field name="zip" placeholder="ZIP" style="width: 20%%" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}"
                                        required="1"/>
                                    </div>
                                    <field name="country_id" placeholder="Country" class="oe_no_button"
                                        options='{"no_open": True}' attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}"
                                    required="1"/>
                                </div>
                                <field name="phone" placeholder="e.g. +32.81.81.37.00" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}"/>
                                <field name="mobile" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="email" widget="email" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="website" widget="url" placeholder="e.g. www.openerp.com" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="tax_config_id" invisible="1"/>
                                <field name="tax_office_id" groups="etax.group_etax_manager" widget="selection"/>

                                <field name="application_type" required="1" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="is_approve" invisible="1" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="generate_tin" attrs="{'invisible':True}" />
                                <field name="tin" attrs="{'invisible':[('generate_tin','=',False)], 'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="select_tin" attrs="{'invisible':True}" />
                                <field name="id" invisible="1" />
                                <field name="res_ptnr_id" class="oe_inline" readonly="1" attrs="{'invisible': [('id', '=', False)], 'required': [('id', '!=', False)], 'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="bvn" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}"/>
                                <field name="driver_license" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}"/>
                                <field name="national_id" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}"/>

                            </group>
                            <group>
                                <label for="street" string="Business Address" />
                                <div>
                                    <field name="office_street" placeholder="Street..." attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}"/>
                                    <field name="office_street2" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                    <div class="address_format">
                                        <field name="office_city" placeholder="City" style="width: 40%%" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                        <field name="office_state_id" class="oe_no_button" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}"
                                            placeholder="State" style="width: 37%%" options='{"no_open": True}'
                                         />
                                        <field name="office_zip" placeholder="ZIP" style="width: 20%%" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                    </div>
                                    <field name="office_country_id" placeholder="Country"
                                        class="oe_no_button" options='{"no_open": True}' attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                </div>
                                <field name="function" placeholder="e.g. Sales Director"
                                    attrs="{'invisible': [('is_company','=', True)], 'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="office_phone" placeholder="e.g. +32.81.81.37.00" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="office_mobile" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="fax" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                <field name="rs_ur_id" invisible="1"/>
                                <field name="title" domain="[('domain', '=', 'contact')]}"
                                    options='{"no_open": True}' attrs="{'invisible': [('is_company','=', True)], 'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                            </group>
                        </group>
                        <notebook colspan="4">
                            <page string="Business Details" autofocus="autofocus">
                                <group>
                                    <group>
                                        <field name="origin_state_id" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                        <field name="local_govt_city" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                        <field name="trade_name" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                    </group>
                                    <group>
                                        <field name="self_employed_business" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                        <field name="business_names" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                        <!--<field name="local_govt_city" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />-->
                                    </group>
                                </group>
                                <group>
                                    <h4>
                                        <label
                                            string="Did you receive income from Nigerian properties including land in the current tax year?
"
                                            colspan="2" />
                                    </h4>
                                    <field name="property_income_receive" nolabel="1"
                                        style="width:10%%" required="1" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />

                                    <h4>
                                        <label
                                            string="Did you dispose of any chargeable assets within the current tax year?
"
                                            colspan="2" />
                                    </h4>
                                    <field name="dispose_of_chargeable_assets" nolabel="1"
                                        style="width:10%%" required="1" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                    <h4>
                                        <label
                                            string="Did you receive any Interests from any Nigerian banks, building Societies, and Trusts?
"
                                            colspan="2" />
                                    </h4>
                                    <field name="interest_income_receive" nolabel="1"
                                        style="width:10%%" required="1" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                    <h4>
                                        <label
                                            string="Did you receive any Dividends within the current tax Year?
"
                                            colspan="2" />
                                    </h4>
                                    <field name="dividend_income_receive" nolabel="1"
                                        style="width:10%%" required="1" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                    <h4>
                                        <label string="Was you income over N300,000 ?
" colspan="2" />
                                    </h4>
                                    <field name="over_income_receive" nolabel="1" style="width:10%%"
                                        required="1" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                    <h4>
                                        <label
                                            string="Did you receive any Nigerian Pensions, Annuities, or State Benefits, for example, State Pension, Occupational Pension, Retirement Annuity, Disability Benefits ?
"
                                            colspan="2" />
                                    </h4>
                                    <field name="pasb_income_receive" nolabel="1" style="width:10%%"
                                        required="1" attrs="{'readonly': [('approve_state','in', ['review','approve','rejected'])]}" />
                                </group>
                                <label for="reject"  attrs="{'invisible': [('approve_state','in', ['new','approve','rejected'])]}"/>
                                <field name="reject"  attrs="{'invisible': [('approve_state','in', ['new','approve','rejected'])]}"/>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
									<field name="message_follower_ids" widget="mail_followers"/>
									<field name="message_ids" widget="mail_thread"/>
							</div>
                </form>
            </field>
        </record>



